{
  "name": "@highoutputventures/insignia-sdk",
  "version": "0.2.4",
  "description": "Insignia Identity Management NodeJS SDK",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run build:nodejs && rimraf dist && npm run build:umd && npm run build:umd:min && npm run build:esm",
    "build:nodejs": "npm run clean && tsc --project tsconfig.build.json",
    "build:umd": "cross-env NODE_ENV=development ROLLUP_FORMAT=umd rollup -c -o dist/umd/index.js",
    "build:umd:min": "cross-env NODE_ENV=production ROLLUP_FORMAT=umd rollup -c -o dist/umd/index.min.js",
    "build:esm": "cross-env NODE_ENV=development ROLLUP_FORMAT=es rollup -c -o dist/esm/index.js",
    "clean": "rimraf build/",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix --cache",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.build.json TS_NODE_FILES=true mocha ./test/{unit,integration}/**/*.spec.ts",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --project tsconfig.eslint.json --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HighOutputVentures/insignia-sdk.git"
  },
  "author": "High Output Ventures",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HighOutputVentures/insignia-sdk/issues"
  },
  "peerDependencies": {
    "@types/socket.io-client": "^1.4.36",
    "rimraf": "^3.0.2",
    "socket.io-client": "^4.0.1",
    "typescript": "^4.2.3"
  },
  "homepage": "https://github.com/HighOutputVentures/insignia-sdk#readme",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^6.1.0",
    "@rollup/plugin-typescript": "4.1.2",
    "@types/chai": "^4.2.17",
    "@types/chai-as-promised": "^7.1.3",
    "@types/chance": "^1.1.1",
    "@types/koa": "^2.13.1",
    "@types/mocha": "^8.2.2",
    "@types/nock": "^11.1.0",
    "@types/node": "^12.20.12",
    "@types/node-fetch": "^2.5.10",
    "@types/ramda": "^0.26.21",
    "@types/sinon": "^10.0.0",
    "@types/socket.io-client": "^1.4.36",
    "@types/url-parse": "^1.4.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "builtin-modules": "^3.2.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.6",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.20.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-node": "^8.8.2",
    "typescript": "^4.2.3",
    "typescript-bundle": "^1.0.17"
  },
  "dependencies": {
    "@highoutput/error": "^0.1.9",
    "@highoutput/logger": "^0.4.0",
    "chance": "^1.1.7",
    "highoutput-auth": "^3.0.1",
    "highoutput-utilities": "0.0.10",
    "koa": "^2.13.1",
    "nock": "^13.0.11",
    "node-fetch": "^2.6.1",
    "ramda": "^0.27.1",
    "sinon": "^10.0.0",
    "socket.io-client": "^4.0.1",
    "try-to-catch": "^3.0.0",
    "url-parse": "^1.5.1",
    "uuid": "^8.3.2"
  }
}
