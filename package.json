{
  "name": "insignia-nodejs-sdk",
  "version": "0.1.0",
  "description": "Insignia Identity Management NodeJS SDK",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./build",
    "clean": "rimraf build/",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix --cache",
    "preinstall": "npm install && npm run build",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.json TS_NODE_FILES=true HTTP_PORT=3000 DEBUG=info*,warn* mocha ./test/{unit,integration}/**/*.spec.ts",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HighOutputVentures/insignia-sdk.git"
  },
  "author": "Highoutput Ventures",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HighOutputVentures/insignia-sdk/issues"
  },
  "homepage": "https://github.com/HighOutputVentures/insignia-sdk#readme",
  "devDependencies": {
    "@types/chai": "^4.2.17",
    "@types/chai-as-promised": "^7.1.3",
    "@types/chance": "^1.1.1",
    "@types/mocha": "^8.2.2",
    "@types/nock": "^11.1.0",
    "@types/node": "^15.0.0",
    "@types/node-fetch": "^2.5.10",
    "@types/ramda": "^0.27.40",
    "@types/sinon": "^10.0.0",
    "@types/socket.io-client": "^1.4.36",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.6",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "@highoutput/logger": "^0.4.0",
    "chance": "^1.1.7",
    "highoutput-utilities": "0.0.10",
    "nock": "^13.0.11",
    "node-fetch": "^2.6.1",
    "ramda": "^0.27.1",
    "sinon": "^10.0.0",
    "socket.io-client": "^4.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
